{
    "compilerOptions": {
        "allowJs": true,
        "target": "ESNext",
        "useDefineForClassFields": false,
        "module": "esnext",
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "strict": false,
        "sourceMap": true,
        "importHelpers": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": [
            "esnext",
            "dom",
            "dom.iterable"
        ],
        "skipLibCheck": true,
    },
}
